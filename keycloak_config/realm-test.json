{
  "realm": "throttling-test",
  "enabled": true,
  "displayName": "Throttling Test Realm",

  "roles": {
    "realm": [
      { "name": "user" },
      { "name": "admin" }
    ]
  },

  "clients": [
    {
      "clientId": "throttling-api",
      "enabled": true,
      "publicClient": false,
      "secret": "test-client-secret",

      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": false,

      "protocol": "openid-connect",
      "defaultClientScopes": ["profile", "email", "roles"]
    }
  ],

  "users": [
    {
      "username": "test-user",
      "enabled": true,

      "firstName": "Test",
      "lastName": "User",

      "email": "test-user@example.com",
      "emailVerified": true,

      "credentials": [
        {
          "type": "password",
          "value": "test-password",
          "temporary": false
        }
      ],

      "requiredActions": [],
      "realmRoles": ["user"]
    },
    {
      "username": "admin-user",
      "enabled": true,

      "firstName": "Admin",
      "lastName": "User",

      "email": "admin-user@example.com",
      "emailVerified": true,

      "credentials": [
        {
          "type": "password",
          "value": "admin-password",
          "temporary": false
        }
      ],

      "requiredActions": [],
      "realmRoles": ["admin"]
    }
  ]
}
